{
	"name": "bilibili-live-helper",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"docs:dev": "npx vitepress dev docs",
		"docs:build": "npx vitepress build docs",
		"docs:serve": "npx vitepress serve docs",
