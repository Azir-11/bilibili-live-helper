		"@unocss/transformer-directives": "^0.45.1",
		"@vitejs/plugin-vue": "^3.0.0",
		"commitlint": "^17.0.3",
		"eslint": "^8.0.1",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-vue": "^9.3.0",
		"husky": "^8.0.1",
		"naive-ui": "^2.32.1",
		"pinia": "^2.0.17",
		"sass": "^1.54.0",
		"typescript": "^4.6.4",
		"unocss": "^0.45.1",
